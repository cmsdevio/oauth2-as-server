{
  "server": {},
  "connections": {
    "$filter": "env",
    "production": [
      {
        "port": 9005,
        "labels": ["oauth2"],
        "routes": { "cors": true }
      }
    ],
    "$default": [
      {
        "port": 9005,
        "labels": ["oauth2"],
        "routes": { "cors": true }
      }
    ]
  },
  "registrations": [
    {
      "plugin": {
        "register": "good",
        "options": {
          "reporters": {
            "console": [
              {
                "module": "good-squeeze",
                "name": "Squeeze",
                "args": [{
                  "log": "*",
                  "request": "*",
                  "response": ["oauth2-*", "ui-*"]
                }]
              },
              {
                "module": "good-console"
              },
              "stdout"
            ]
          }
        }
      }
    },
    {
      "plugin": "vision"
    },
    {
      "plugin": "inert"
    },
    {
      "plugin": "lout"
    },
    {
      "plugin": "hapi-auth-cookie"
    },
    {
      "plugin": "hapi-auth-bearer-token"
    },
    {
      "plugin": "./lib/modules/authServer/index",
      "options": {
        "select": ["oauth2"],
        "routes": {
          "prefix": "/oauth2"
        }
      }
    }
  ]
}