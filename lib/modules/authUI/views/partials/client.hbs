<div class="container theme-showcase" role="main">
    <div class="page-header">
        <h1>List Clients</h1>
    </div>
    <div class="row">
        <div class="col-md-12">
            <table class="table table-striped">
                <thead>
                <tr>
                    <th>Client ID</th>
                    <th>Client Secret</th>
                    <th>Redirect URIs</th>
                </tr>
                </thead>
                <tbody>
                {{#each clients}}
                    <tr>
                        <td>{{client_id}}</td>
                        <td>{{client_secret}}</td>
                        <td>{{redirect_uris}}</td>
                    </tr>
                {{/each}}
                </tbody>
            </table>
        </div>
    </div>
</div>