<div class="container theme-showcase" role="main">
    <div class="page-header">
        <h1>List Clients</h1>
    </div>
    <div class="row">
        <div class="col-md-12">
            <table class="table table-striped">
                <thead>
                <tr>
                    <th>Client ID</th>
                    <th>Client Secret</th>
                    <th>Redirect URIs</th>
                    <th>Actions</th>
                </tr>
                </thead>
                <tbody>
                {{#each clients}}
                    {{> client}}
                {{/each}}
                </tbody>
            </table>
            <p>
                <a class="btn btn-large btn-info" href="/ui/addclient">Add</a>
            </p>
        </div>
    </div>
</div>